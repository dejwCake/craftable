{
    "name": "dejwcake/craftable",
    "description": "Administration toolkit for Laravel 11 - starting point for developing administration interface",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "craftable",
        "crud",
        "brackets",
        "admin",
        "administration",
        "toolkit",
        "framework",
        "cms",
        "laravel"
    ],
    "authors": [
        {
            "name": "David Běhal",
            "email": "david.behal@dejw.sk",
            "homepage": "https://www.dejw.sk",
            "role": "Developer"
        },
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "Richard Dominik",
            "email": "richard.dominik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "dejwcake/admin-auth": "1.x-dev",
        "dejwcake/admin-listing": "1.x-dev",
        "dejwcake/admin-translations": "1.x-dev",
        "dejwcake/admin-ui": "1.x-dev",
        "dejwcake/advanced-logger": "1.x-dev",
        "dejwcake/craftable-media": "1.x-dev",
        "dejwcake/craftable-translatable": "1.x-dev",
        "illuminate/support": "^12.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-backup": "^9.2.7",
        "spatie/laravel-permission": "^6.15"
    },
    "require-dev": {
        "larastan/larastan": "^3.1",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.5.9"
    },
    "autoload": {
        "psr-4": {
            "Brackets\\Craftable\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\Craftable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\Craftable\\CraftableServiceProvider"
            ]
        }
    },
    "branch-alias": {
        "dev-main": "1.0-dev"
    }
}
